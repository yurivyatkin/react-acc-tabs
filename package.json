{
  "name": "react-acc-tabs",
  "version": "0.0.1",
  "description": "A collection of headless React components for rendering accessible tabbed UIs.",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "install:demo": "npm install --prefix demo",
    "install:lib": "npm install --prefix lib",
    "install": "npm run install:demo && npm run install:lib",
    "demo": "npm start --prefix demo",
    "test": "npm test --prefix lib",
    "storybook": "npm run storybook --prefix lib",
    "build-storybook": "npm run build-storybook --prefix lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurivyatkin/react-acc-tabs.git"
  },
  "keywords": ["accessible", "tabs", "react"],
  "author": "Yuri Vyatkin",
  "license": "UNLICENSE",
  "bugs": {
    "url": "https://github.com/yurivyatkin/react-acc-tabs/issues"
  },
  "homepage": "https://github.com/yurivyatkin/react-acc-tabs#readme"
}
